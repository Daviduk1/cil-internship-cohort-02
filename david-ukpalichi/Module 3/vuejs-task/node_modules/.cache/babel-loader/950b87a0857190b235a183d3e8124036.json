{"ast":null,"code":"import axios from 'axios';\nimport ContactCard from '@/components/ContactCard.vue';\nexport default {\n  components: {\n    ContactCard\n  },\n  name: 'HomeView',\n\n  data() {\n    return {\n      users: []\n    };\n  },\n\n  mounted() {\n    axios.get('https://jsonplaceholder.typicode.com/users').then(res => {\n      this.users = res.data;\n    });\n    let newUser = this.$route.params.newUser;\n\n    if (newUser) {\n      console.log(newUser);\n    }\n  },\n\n  methods: {\n    editUser(id) {\n      console.log('I am editing contact with id', id);\n    },\n\n    deleteUser(id) {\n      let filteredUsers = this.users.filter(user => user.id !== id);\n      this.users = filteredUsers;\n    },\n\n    demo(user) {\n      console.log(user);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkBE,OAAOA,KAAP,MAAkB,OAAlB;AACF,OAAOC,WAAP,MAAwB,8BAAxB;AACE,eAAe;AACfC,YAAU,EAAE;AAAED;AAAF,GADG;AAEbE,MAAI,EAAE,UAFO;;AAGbC,MAAI,GAAE;AACJ,WAAO;AACLC,WAAK,EAAE;AADF,KAAP;AAGD,GAPY;;AAQbC,SAAO,GAAG;AACRN,SAAI,CACDO,GADH,CACQ,4CADR,EAEGC,IAFH,CAEQC,GAAE,IAAK;AACX,WAAKJ,KAAL,GAAaI,GAAG,CAACL,IAAjB;AACD,KAJH;AAME,QAAIM,OAAM,GAAI,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,OAAjC;;AACA,QAAIA,OAAJ,EAAa;AACXG,aAAO,CAACC,GAAR,CAAYJ,OAAZ;AACF;AACH,GAnBY;;AAoBbK,SAAO,EAAE;AACPC,YAAO,CAAGC,EAAH,EAAO;AACZJ,aAAO,CAACC,GAAR,CAAY,8BAAZ,EAA4CG,EAA5C;AACD,KAHM;;AAIPC,cAAS,CAAGD,EAAH,EAAO;AACd,UAAIE,aAAY,GAAI,KAAKd,KAAL,CAAWe,MAAX,CAAkBC,IAAG,IAAKA,IAAI,CAACJ,EAAL,KAAYA,EAAtC,CAApB;AACA,WAAKZ,KAAL,GAAac,aAAb;AACD,KAPM;;AAQPG,QAAG,CAAGD,IAAH,EAAS;AACVR,aAAO,CAACC,GAAR,CAAYO,IAAZ;AACF;;AAVO;AApBI,CAAf","names":["axios","ContactCard","components","name","data","users","mounted","get","then","res","newUser","$route","params","console","log","methods","editUser","id","deleteUser","filteredUsers","filter","user","demo"],"sourceRoot":"","sources":["C:\\Users\\JAMES\\Desktop\\vuejs-task\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <main>\n      <contact-card \n        v-for=\"user in users\" \n        :key=\"user.id\" \n        :contact=\"user\"\n        @edit-contact=\"(id) => editUser(id)\"\n        @delete-contact=\"(id) => deleteUser(id)\"\n        @new-user=\"(user) => demo(user)\"\n      >\n      </contact-card>\n\n    </main>\n  </div>\n</template>\n\n<script>\n  import axios from 'axios'\nimport ContactCard from '@/components/ContactCard.vue'\n  export default {\n  components: { ContactCard },\n    name: 'HomeView',\n    data(){\n      return {\n        users: []\n      }\n    },\n    mounted() {\n      axios\n        .get ('https://jsonplaceholder.typicode.com/users')\n        .then(res => {\n          this.users = res.data\n        })\n\n        let newUser = this.$route.params.newUser\n        if (newUser) {\n          console.log(newUser);\n        }\n    },\n    methods: {\n      editUser (id) {\n        console.log('I am editing contact with id', id);\n      },\n      deleteUser (id) {\n        let filteredUsers = this.users.filter(user => user.id !== id)\n        this.users = filteredUsers\n      },\n      demo (user) {\n        console.log(user);\n      }\n    }\n  }\n</script>\n<style scoped>\n\n.home {\n    min-height:90vh;\n    display: grid;\n    grid-template-rows: 1fr auto;\n}\n  \n.home main {\n    padding: 10px 15%;\n    display: flex;\n    flex-direction: column;\n    gap: 15px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}